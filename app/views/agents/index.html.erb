<h1>Listing agents</h1>

<table>
  <thead>
    <tr>
      <th>Firstname</th>
      <th>Lastname</th>
      <th>Middleinit</th>
      <th>Company</th>
      <th>Phoneh</th>
      <th>Phonew</th>
      <th>Phonec</th>
      <th>Email</th>
      <th>Mailaddress</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @agents.each do |agent| %>
      <tr>
        <td><%= agent.firstName %></td>
        <td><%= agent.lastName %></td>
        <td><%= agent.middleInit %></td>
        <td><%= agent.company %></td>
        <td><%= agent.phoneH %></td>
        <td><%= agent.phoneW %></td>
        <td><%= agent.phoneC %></td>
        <td><%= agent.email %></td>
        <td><%= agent.mailAddress %></td>
        <td><%= link_to 'Show', agent %></td>
        <td><%= link_to 'Edit', edit_agent_path(agent) %></td>
        <td><%= link_to 'Destroy', agent, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Agent', new_agent_path %>
