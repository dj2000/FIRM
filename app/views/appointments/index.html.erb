<h1>Listing appointments</h1>

<table>
  <thead>
    <tr>
      <th>Insprequest</th>
      <th>Schedstart</th>
      <th>Schedend</th>
      <th>Allday</th>
      <th>Inspector</th>
      <th>Contact</th>
      <th>Inspfee</th>
      <th>Prepaid</th>
      <th>Pmtmethod</th>
      <th>Pmtref</th>
      <th>Notes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @appointments.each do |appointment| %>
      <tr>
        <td><%= appointment.inspRequest_id %></td>
        <td><%= appointment.schedStart %></td>
        <td><%= appointment.schedEnd %></td>
        <td><%= appointment.allDay %></td>
        <td><%= appointment.inspector_id %></td>
        <td><%= appointment.contact %></td>
        <td><%= appointment.inspFee %></td>
        <td><%= appointment.prepaid %></td>
        <td><%= appointment.pmtMethod %></td>
        <td><%= appointment.pmtRef %></td>
        <td><%= appointment.notes %></td>
        <td><%= link_to 'Show', appointment %></td>
        <td><%= link_to 'Edit', edit_appointment_path(appointment) %></td>
        <td><%= link_to 'Destroy', appointment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Appointment', new_appointment_path %>
