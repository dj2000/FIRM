<div class="row">
  <div class='col-sm-12'>
    <div class="form-group">
      <strong>First Name:</strong>
      <%= @client.firstName %>
    </div>
  </div>
</div>
<div class="row">
  <div class='col-sm-12'>
    <div class="form-group">
      <strong>Last Name:</strong>
      <%= @client.lastName %>
    </div>
  </div>
</div>
<div class="row">
  <div class='col-sm-12'>
    <div class="form-group">
      <strong>Middle Initial:</strong>
      <%= @client.middleInit %>
    </div>
  </div>
</div>
<div class="row">
  <div class='col-sm-12'>
    <div class="form-group">
      <strong>Home Number:</strong>
      <%= number_to_phone(@client.phoneH, area_code: true) %>
    </div>
  </div>
</div>
<div class="row">
  <div class='col-sm-12'>
    <div class="form-group">
      <strong>Office Number:</strong>
      <%= number_to_phone(@client.phoneW, area_code: true) %>
    </div>
  </div>
</div>
<div class="row">
  <div class='col-sm-12'>
    <div class="form-group">
      <strong>Cell Number:</strong>
      <%= number_to_phone(@client.phoneC, area_code: true) %>
    </div>
  </div>
</div>
<div class="row">
  <div class='col-sm-12'>
    <div class="form-group">
      <strong>Email:</strong>
      <%= @client.email %>
    </div>
  </div>
</div>
<div class="row">
  <div class='col-sm-12'>
    <div class="form-group">
      <strong>Mail Address:</strong>
      <%= @client.mailAddress %>
    </div>
  </div>
</div>