<h1>Listing crew_skills</h1>

<table>
  <thead>
    <tr>
      <th>Crew</th>
      <th>Skill</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @crew_skills.each do |crew_skill| %>
      <tr>
        <td><%= crew_skill.crew_id %></td>
        <td><%= crew_skill.skill_id %></td>
        <td><%= link_to 'Show', crew_skill %></td>
        <td><%= link_to 'Edit', edit_crew_skill_path(crew_skill) %></td>
        <td><%= link_to 'Destroy', crew_skill, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Crew skill', new_crew_skill_path %>
