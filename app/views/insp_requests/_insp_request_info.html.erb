<fieldset><legend></legend></fieldset>
<div class="row">
	<div class="col-md-2 property">
		<%= label_tag "Property" %>
		<br/>
		<span><%= @insp_request.property.try(:property_select_value) %></span>
		<% if @insp_request.property %>
		<%= link_to property_path(@insp_request.property), remote: true, class: 'link' do %>
		<i class='fa fa-eye'></i> Details
		<% end %>
		<% end %>
	</div>
	<div class="col-md-2 client">
		<%= label_tag "Client" %>
		<br/>
		<span><%= @insp_request.client.try(:name) %></span>
		<% if @insp_request.client %>
		<%= link_to client_path(@insp_request.client), remote: true, class: 'link' do %>
		<i class='fa fa-eye'></i> Details
		<% end %>
		<% end %>
	</div>
	<div class="col-md-2">
		<%= label_tag "Agent" %>
		<br/>
		<%= @insp_request.agent.try(:name) %>
		<% if @insp_request.agent %>
		<%= link_to agent_path(@insp_request.agent), remote: true, class: 'link' do %>
		<i class='fa fa-eye'></i> Details
		<% end %>
		<% end %>
	</div>
	<div class="col-md-2">
		<%= label_tag "Inspection" %>
		<br/>
		<%= @bid.try(:inspection).try(:name) %>
		<% if @bid.try(:inspection) %>
			<%= link_to inspection_path(@bid.try(:inspection)), remote: true, class: 'link' do %>
				<i class='fa fa-eye'></i> Details
			<% end %>
		<% end %>
	</div>
	<div class="col-md-2">
		<%= label_tag "Call History" %>
		<br/>
		<% if @bid.try(:comm_histories) %>
			<%= link_to comm_histories_path(id: @bid), remote: true, class: 'link' do %>
				<i class='fa fa-eye'></i> Details
			<% end %>
		<% end %>
	</div>
</div>
<br/>