<fieldset><legend></legend></fieldset>
<div class="row">
	<div class="col-md-4">
		<%= label_tag "Property" %>
		<br/>
		<%= @insp_request.property.try(:street) %>
		<% if @insp_request.property %>
		<%= link_to property_path(@insp_request.property), remote: true, class: 'link' do %>
		<i class='fa fa-eye'></i> Details
		<% end %>
		<% end %>
	</div>
	<div class="col-md-4">
		<%= label_tag "Client" %>
		<br/>
		<%= @insp_request.client.try(:name) %>
		<% if @insp_request.client %>
		<%= link_to client_path(@insp_request.client), remote: true, class: 'link' do %>
		<i class='fa fa-eye'></i> Details
		<% end %>
		<% end %>
	</div>
	<div class="col-md-4">
		<%= label_tag "Agent" %>
		<br/>
		<%= @insp_request.agent.try(:name) %>
		<% if @insp_request.agent %>
		<%= link_to agent_path(@insp_request.agent), remote: true, class: 'link' do %>
		<i class='fa fa-eye'></i> Details
		<% end %>
		<% end %>
	</div>
</div>
<br/>