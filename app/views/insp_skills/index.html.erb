<h1>Listing insp_skills</h1>

<table>
  <thead>
    <tr>
      <th>Inspector</th>
      <th>Skill</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @insp_skills.each do |insp_skill| %>
      <tr>
        <td><%= insp_skill.inspector_id %></td>
        <td><%= insp_skill.skill_id %></td>
        <td><%= link_to 'Show', insp_skill %></td>
        <td><%= link_to 'Edit', edit_insp_skill_path(insp_skill) %></td>
        <td><%= link_to 'Destroy', insp_skill, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Insp skill', new_insp_skill_path %>
