<h1>Listing pay_plans</h1>

<table>
  <thead>
    <tr>
      <th>Jobminamt</th>
      <th>Jobmaxamt</th>
      <th>Pmt1pcnt</th>
      <th>Pmt2pcnt</th>
      <th>Pmt3pcnt</th>
      <th>Pmt4pcnt</th>
      <th>Pmt5pcnt</th>
      <th>Pmt1desc</th>
      <th>Pmt2desc</th>
      <th>Pmt3desc</th>
      <th>Pmt4desc</th>
      <th>Pmt5desc</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pay_plans.each do |pay_plan| %>
      <tr>
        <td><%= pay_plan.jobMinAmt %></td>
        <td><%= pay_plan.jobMaxAmt %></td>
        <td><%= pay_plan.pmt1Pcnt %></td>
        <td><%= pay_plan.pmt2Pcnt %></td>
        <td><%= pay_plan.pmt3Pcnt %></td>
        <td><%= pay_plan.pmt4Pcnt %></td>
        <td><%= pay_plan.pmt5Pcnt %></td>
        <td><%= pay_plan.pmt1Desc %></td>
        <td><%= pay_plan.pmt2Desc %></td>
        <td><%= pay_plan.pmt3Desc %></td>
        <td><%= pay_plan.pmt4Desc %></td>
        <td><%= pay_plan.pmt5Desc %></td>
        <td><%= link_to 'Show', pay_plan %></td>
        <td><%= link_to 'Edit', edit_pay_plan_path(pay_plan) %></td>
        <td><%= link_to 'Destroy', pay_plan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pay plan', new_pay_plan_path %>
