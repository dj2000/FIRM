<h1>Listing proj_insps</h1>

<table>
  <thead>
    <tr>
      <th>Project</th>
      <th>Reference</th>
      <th>Scheduledate</th>
      <th>Inspectby</th>
      <th>Completedate</th>
      <th>Result</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @proj_insps.each do |proj_insp| %>
      <tr>
        <td><%= proj_insp.project_id %></td>
        <td><%= proj_insp.reference %></td>
        <td><%= proj_insp.scheduleDate %></td>
        <td><%= proj_insp.inspectBy %></td>
        <td><%= proj_insp.completeDate %></td>
        <td><%= proj_insp.result %></td>
        <td><%= link_to 'Show', proj_insp %></td>
        <td><%= link_to 'Edit', edit_proj_insp_path(proj_insp) %></td>
        <td><%= link_to 'Destroy', proj_insp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

