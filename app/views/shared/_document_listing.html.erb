<% f.object.try(:bid).try(:inspection).try(:documents).try(:email_documents).each do |document| %>
  <%= link_to document.attachment_file_name, document.attachment.url, class: 'blue-link' %>
  <%= link_to '', document_path(document, parent_id: f.object.bid.inspection, attachable_type: f.object.bid.inspection.class, f: f), data: {:confirm => 'Are you sure?'}, :method => :delete, remote: true, class: "glyphicon glyphicon-trash" %>
  <br/>
<% end %>